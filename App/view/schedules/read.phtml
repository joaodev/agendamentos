                    $("#modalMid").modal('show');     

                    await $.ajax({
                        type: 'post',
                        url: "<?php echo baseUrl; ?>agendamentos/alterar-situacao",
                        data: { 
                            id: id,
                            target: '<?php echo $this->getTarget(); ?>'
                        },
                        async: true
                    }).done(function (data) {
                        $("#modalMidResult").html(data);
                        $("#modalMidLoader").hide();
                    }).fail(function () {
                        showInternalErrorAlert();
                        $("#modalMidLoader").hide();
                    });
                }
            </script>
        <?php endif; ?>
        <?php if ($canUpdate && ($entity['status'] != '2' || $isAdmin) && $entity['deleted'] == 0): ?>
            <script>
                async function updateSchedule(id) {
                    $("#baseLoader").show();
                    await $.ajax({
                        type: 'post',
                        url: "<?php echo baseUrl; ?>agendamentos/editar",
                        data: {
                            id:id, 
                            target: '<?php echo $this->getTarget(); ?>'
                        },
                        async: true
                    }).done(function(data) {
                        $("#sis_content").html(data);
                        $("#baseLoader").fadeOut();
                    }).fail(function() {
                        showInternalErrorAlert();
                        $("#baseLoader").fadeOut();
                    });
                }
            </script>
        <?php endif; ?>
        <?php if ($canDelete && ($entity['status'] != '2' || $isAdmin) && $entity['deleted'] == 0): ?>
            <script>
                function deleteSchedule(id) {
                    $("#modalXL").modal('hide');
                    $("#modalDeleteScheduleTitle").html('<i class="fa fa-trash pr-2"></i> Excluir Agendamento');
                    $("#modalDeleteScheduleLoader").hide();
                    $("#modalDeleteSchedule").modal('show');
                    $("#modalDeleteScheduleResult").html('<button type="button" class="btn btn-danger btnRadius" title="Confirmar Exclusão" onclick="runDelete(\''+id+'\');"><i class="fa fa-trash"></i> CONFIRMAR E EXCLUIR</button>');
                }
                
                async function runDelete(id) {
                    if (id !== null) {
                        
                        let status_mail = null;
                        if ($('#schedule_delete_notification').is(':checked')) {
                            status_mail = $("#schedule_delete_notification").val();
                        } 
                       
                        $("#modalDeleteScheduleLoader").show();

                        await $.ajax({
                            type: "POST",
                            url: "<?php echo baseUrl; ?>agendamentos/excluir",
                            data: {
                                id: id, 
                                schedule_id: '<?php echo $_POST['id']; ?>',
                                send_mail: status_mail,
                                target: '<?php echo $this->getTarget(); ?>'
                            },
                            dataType: 'json',
                            async: true
                        }).done(async function(data) {
                            showAlert(data.type, data.title, data.msg, data.pos);
                            $("#modalDeleteSchedule").modal('hide');
                            $("#modalXL").modal('hide');
                            await openModule('agendamentos');
                        }).fail(function() {
                            showInternalErrorAlert();
                            $("#modalDeleteScheduleLoader").hide();
                        });
                    }
                }
            </script>
        <?php endif; ?>
    <?php endif; ?>
<?php endif; ?>