                    $("#modalRestore").modal('show');
                    $("#modalRestoreResult").html('<button type="button" class="btn btn-primary btnRadius" title="Confirmar" onclick="runRestore(\''+id+'\');"><i class="fa fa-check"></i> CONFIRMAR</button>');
                }

                async function runRestore(id) {
                    if (id !== null) {
                        $("#modalRestoreLoader").show();

                        let status_mail = null;
                        if ($('#basic_restore_notification').is(':checked')) {
                            status_mail = $("#basic_restore_notification").val();
                        } 

                        await $.ajax({
                            type: "POST",
                            url: "<?php echo baseUrl; ?>agendamentos/restaurar",
                            data: {
                                id: id, 
                                send_mail: status_mail,
                                target: '<?php echo $this->getTarget(); ?>'
                            },
                            dataType: 'json',
                            async: true
                        }).done(async function(data) {
                            showAlert(data.type, data.title, data.msg, data.pos);
                            $("#modalRestore").modal('hide');
                            await openScheduleDetails(id);
                        }).fail(function() {
                            showInternalErrorAlert();
                            $("#modalRestoreLoader").hide();
                        });
                    }
                }
